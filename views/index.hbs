        <form action="/" method="get">
            <div class="row">
                <div class="large-12 columns">
                    <div class="row collapse">
                        <div class="small-10 columns">
                            <input type="text" name="username" placeholder="Github username" value="{{ username }}">
                        </div>
                        <div class="small-2 columns">
                            <button type="submit" class="button postfix">Check</button>
                        </div>
                    </div>
                </div>
            </div>
        </form>

        <div id="results">
            {{#if error}}
                {{#if username}}
                    {{> error}}
                {{/if}}
            {{else}}
                {{> prs}}
            {{/if}}
        </div>

        <div id="open-issues"></div>

        <footer>
            <div class="large-12 columns text-center">
                <p class="disclaimer">Disclaimer: This site is fan made and not affiliated with <a href="https://hacktoberfest.digitalocean.com/" target="_blank">Hacktoberfest</a>.</p>
            </div>
        </footer>
